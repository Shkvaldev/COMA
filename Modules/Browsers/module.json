{
    "name": "Browser",
    "desc": "Browser launcher module",
    "triggers": {
        "открой браузер": [
            {"sdkFunc": "say", "args": ["Открываю браузер!"]},
            {"sdkFunc": "openBrowser", "args": []}
        ],
        "закрой браузер": [
            {"sdkFunc": "say", "args": ["Закрываю браузер!"]},
            {"sdkFunc": "closeBrowser", "args": []}
        ],
        "найди": [
            {"sdkFunc": "say", "args": ["Ищу!"]},
            {"sdkFunc": "engoogle", "args": ["$speech"]}
        ]
    }
}